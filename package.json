{
    "name": "language-forth",
    "publisher": "fttx",
    "displayName": "language-forth",
    "description": "Forth language support in VSCode with objects.fs highlighting added from Gforth!",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "forth",
            "aliases": ["Forth", "forth"],
            "extensions": ["f", "frt","fth","fs","4th","forth"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "forth",
            "scopeName": "source.forth",
            "path": "./syntaxes/forth.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fttx/language-forth"
    }
}